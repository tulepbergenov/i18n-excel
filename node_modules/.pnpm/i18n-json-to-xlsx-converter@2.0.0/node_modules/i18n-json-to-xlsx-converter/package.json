{
  "name": "i18n-json-to-xlsx-converter",
  "version": "2.0.0",
  "description": "i18n JSON to XLSX Converter is a CLI tool helps you convert your JSON files to XLSX format easily.",
  "author": "Osman Fikret Ceylan",
  "license": "MIT",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "start": "node dist/index.d.ts",
    "build": "tsc",
    "test": "jest --config jestconfig.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ofcyln/i18n-json-to-xlsx-converter.git"
  },
  "keywords": [
    "translation",
    "file conversion",
    "json",
    "xlsx",
    "dot notation",
    "nested object",
    "from an excel file to multiple json files",
    "convert files"
  ],
  "dependencies": {
    "chalk": "^4.1.1",
    "exceljs": "^4.2.1",
    "flat": "^5.0.2",
    "fs-extra": "^10.0.0",
    "read-excel-file": "^5.2.7"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.2",
    "execa": "^5.1.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.3.5"
  }
}
