#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules/i18n-json-to-xlsx-converter/dist/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules/i18n-json-to-xlsx-converter/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules/i18n-json-to-xlsx-converter/dist/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules/i18n-json-to-xlsx-converter/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/i18n-json-to-xlsx-converter@2.0.0/node_modules:/Users/daulettulepbergenov/Documents/projects/i18n-excel/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../i18n-json-to-xlsx-converter/dist/index.js" "$@"
else
  exec node  "$basedir/../i18n-json-to-xlsx-converter/dist/index.js" "$@"
fi
